"*":
  "atom-beautify":
    executables:
      autopep8: {}
  "atomic-emacs":
    alwaysUseKillRing: true
  core:
    excludeVcsIgnoredPaths: false
    packagesWithKeymapsDisabled: [
      "highlight-line"
      "atom-beautify"
    ]
    telemetryConsent: "no"
    uriHandlerRegistration: "never"
  "disable-keybindings":
    exceptCommunityPackages: [
      "emacs-plus"
      "atomic-emacs"
    ]
    prefixKeys: [
      "ctrl-k"
    ]
  editor:
    autoIndentOnPaste: false
    fontFamily: "Ricty Diminished Discord, Menlo, Consolas, DejaVu Sans Mono, monospace"
    showIndentGuide: true
    softWrap: true
    tabLength: 4
  "exception-reporting":
    userId: "b5483ec8-dc77-4b2d-bf24-af83e15170bc"
  "highlight-line":
    enableBackgroundColor: false
    enableSelectionBorder: true
    enableUnderline: true
    underline: "dashed"
  "platformio-ide-terminal":
    core:
      shell: "F:\\Tool\\Git\\bin\\bash.exe"
    style: {}
    toggles:
      autoClose: true
